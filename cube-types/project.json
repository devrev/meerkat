{
  "name": "cube-types",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "cube-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/cube-types",
        "main": "cube-types/src/index.ts",
        "tsConfig": "cube-types/tsconfig.lib.json",
        "assets": ["cube-types/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs cube-types {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["cube-types/**/*.ts", "cube-types/package.json"]
      }
    }
  },
  "tags": []
}
